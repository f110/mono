{{define "main_content"}}
<div class="Container">
  <div class="Content">
    <img class="Error-gopher" src="/static/img/gopher-airplane.svg" alt="The Go Gopher">
    <h3 class="Error-message">{{ .Message }}</h3>
    {{if .SecondaryMessage}}
      <p class="Error-message">{{.SecondaryMessage}}</p>
    {{end}}
  </div>
</div>
{{end}}
